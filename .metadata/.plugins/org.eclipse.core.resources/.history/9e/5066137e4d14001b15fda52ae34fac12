//Desc: prime numbers upto a particular integer 

import java.util.*;
public class Prime {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter n:");
		int n=sc.nextInt();
		int temp=0;
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=i;j++) {
			if(i%j==0) 
				temp++;						
			}
			if(temp==2) 
				System.out.println(i);
						
		}
	}

}
